{
  "name": "brainymath",
  "version": "1.0.0",
  "description": "Educational Platform",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon --exitcrash server.js",
    "client": "cd client && npm start",
    "dev:all": "concurrently \"npm run dev\" \"npm run client\"",
    "test": "jest --watchAll",
    "init-db": "node -e \"require('./src/utils/migrations').initializeDatabase()\"",
    "seed": "node -e \"require('./src/utils/seed').seedData()\"",
    "setup": "npm run init-db && npm run seed",
    "setup-thumbnails": "node src/utils/setupTempDir.js",
    "migrate-thumbnails": "node src/utils/migrations/addThumbnailPath.js",
    "create-whiteboard-table": "node src/utils/migrations/createWhiteboardTable.js",
    "migrate": "knex migrate:latest",
    "rollback": "knex migrate:rollback",
    "clean": "taskkill /F /IM node.exe",
    "lint": "eslint ."
  },
  "dependencies": {
    "axios": "^1.6.7",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "concurrently": "^9.1.2",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "ejs": "^3.1.9",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "express-validator": "^7.0.1",
    "fabric": "^6.6.4",
    "helmet": "^7.2.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "mathjs": "^14.4.0",
    "moment": "^2.30.1",
    "mongoose": "^8.1.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.14.0",
    "natural": "^8.0.1",
    "openai": "^4.96.0",
    "puppeteer": "^24.7.2",
    "sharp": "^0.34.1",
    "socket.io": "^4.8.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "swagger-ui-themes": "^3.0.1",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.4"
  }
}
